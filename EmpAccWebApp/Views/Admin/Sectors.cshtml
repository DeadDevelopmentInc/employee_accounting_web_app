@model IEnumerable<EmpAccWebApp.Models.AdminViewModels.SectorViewModel>

@{
    Layout = "~/Views/Admin/_Layout.cshtml";
    ViewData["Title"] = "Sectors";
}

<p>
    @Html.ActionLink("Create new sector", "CreateSector", "Admin", null, null)
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(Model => Model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(Model => Model.BranchName)
            </th>
            <th>
                @Html.DisplayNameFor(Model => Model.HeadFullName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BranchName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HeadFullName)
            </td>
            <td>
                <a> @Html.ActionLink("Edit", "EditDSector", "Admin", new { id = item.Id }, null)</a>
                <a> @Html.ActionLink("Delete", "DeleteSector", "Admin", new { id = item.Id }, null)</a>
            </td>
        </tr>
        }
    </tbody>
</table>
